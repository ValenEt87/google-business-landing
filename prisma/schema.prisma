generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String     @id @default(cuid())
  email      String     @unique
  name       String?
  image      String?
  businesses Business[]
}

model Business {
  id       String   @id @default(cuid())
  title    String
  phone    String?
  address  String?
  category String?
  hours    Json?
  website  String?
  userId   String
  user     User     @relation(fields: [userId], references: [id])
}
